<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="150.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.RenameTaskListController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="label" layoutX="22.0" layoutY="32.0" text="New Title" />
            <TextArea fx:id="textArea" layoutX="94.0" layoutY="32.0" prefHeight="63.0" prefWidth="200.0" promptText="Enter a new title of your task list" />
            <Button fx:id="saveButton" layoutX="253.0" layoutY="101.0" mnemonicParsing="false" onAction="#renameClicked" onMouseClicked="#renameClicked" text="Save" />
      </children>
    </AnchorPane>
  </children>
</VBox>
